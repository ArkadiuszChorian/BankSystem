@using BankService
@{
    ViewData["Title"] = "Overview";
}

@{
    var accounts = (List<Account>)Model;
    foreach (var account in accounts)
    {
        <div>
            Konto: @account.Id, Bilans: @account.Balance
            <a asp-area="" asp-controller="Home" asp-action="History" asp-route-accountId="@account.Id">History</a>
        </div>
    }
}
